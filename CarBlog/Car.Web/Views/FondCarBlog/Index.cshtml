@model Car.Web.ViewModel.SeachFormCarViewModel
@using Car.Web.DataContext.Enum;
@{
     var ShowImage = "~/Files/CarBand/Images/";
}

@foreach(var CarBand in Model.carBands){
    <img src="@(ShowImage + CarBand.BandImagePath)" asp-append-version="true" width="50" height="50">
}